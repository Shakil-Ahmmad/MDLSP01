@{
    ViewBag.Title = "School List";
}

<center> <h1>SchoolList</h1> </center>

<div class="table-responsive">
    <table class="table text-nowrap">
        <thead>
            <tr>
                <th>School Name</th>
                <th class="col-md-2">Contact No</th>
                <th class="col-md-2">Total Student</th>
                <th class="col-md-2">Total Teacher</th>
                <th class="col-md-2">Status</th>
                <th class="text-center" style="width: 20px;">Action<i class="icon-arrow-down12"></i></th>
            </tr>
        </thead>
        <tbody>
            <tr class="active border-double">
                <td colspan="5">Today</td>
                <td class="text-right">
                    <span class="progress-meter" id="today-progress" data-progress="30"><svg width="20" height="20"><g transform="translate(10,10)"><g class="progress-meter"><path d="M0,8A8,8 0 1,1 0,-8A8,8 0 1,1 0,8Z" style="fill: rgb(255, 255, 255); stroke: rgb(121, 134, 203); stroke-width: 1.5;"></path><path d="M4.898587196589413e-16,-8A8,8 0 0,1 7.608452130361228,2.472135954999579L0,0Z" style="fill: rgb(121, 134, 203);"></path></g></g></svg></span>
                </td>
            </tr>
            <tr>
                <td>
                    <div class="media-left media-middle">
                        <a href="#"><img src="assets/images/brands/facebook.png" class="img-circle img-xs" alt=""></a>
                    </div>
                    <div class="media-left">
                        <div class=""><a href="#" class="text-default text-semibold">Amin Model Towen School </a></div>
                        <div class="text-muted text-size-small">
                            <span class="status-mark border-blue position-left"></span>
                            02:00 - 03:00
                        </div>
                    </div>
                </td>
                <td><span class="text-muted">016xxxxxx</span></td>
                <td><span class="text-success-600"><i class="icon-stats-growth2 position-left"></i> 2.43%</span></td>
                <td><h6 class="text-semibold">$5,489</h6></td>
                <td><span class="label bg-blue">Active</span></td>
                <td class="text-center">
                    <ul class="icons-list">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="icon-menu7"></i></a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#"><i class="icon-file-stats"></i>Inactive</a></li>
                                <li><a href="#"><i class="icon-file-text2"></i> View Details</a></li>
                            </ul>
                        </li>
                    </ul>
                </td>
            </tr>
            <tr>
                <td>
                    <div class="media-left media-middle">
                        <a href="#"><img src="assets/images/brands/youtube.png" class="img-circle img-xs" alt=""></a>
                    </div>
                    <div class="media-left">
                        <div class=""><a href="#" class="text-default text-semibold">Amin Model Towen School 2</a></div>
                        <div class="text-muted text-size-small">
                            <span class="status-mark border-danger position-left"></span>
                            13:00 - 14:00
                        </div>
                    </div>
                </td>
                <td><span class="text-muted">016xxxxxx</span></td>
                <td><span class="text-success-600"><i class="icon-stats-growth2 position-left"></i> 3.12%</span></td>
                <td><h6 class="text-semibold">$2,592</h6></td>
                <td><span class="label bg-danger">Inative</span></td>
                <td class="text-center">
                    <ul class="icons-list">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-menu7"></i></a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a href="#"><i class="icon-file-stats"></i>Ative</a></li>
                                <li><a href="#"><i class="icon-file-text2"></i> View Details</a></li>
                                @*<li><a href="#"><i class="icon-file-locked"></i> Disable campaign</a></li>*@

                            </ul>
                        </li>
                    </ul>
                </td>
            </tr>
            
            

            <tr class="active border-double">
                <td colspan="5">Yesterday</td>
                <td class="text-right">
                    <span class="progress-meter" id="yesterday-progress" data-progress="65"><svg width="20" height="20"><g transform="translate(10,10)"><g class="progress-meter"><path d="M0,8A8,8 0 1,1 0,-8A8,8 0 1,1 0,8Z" style="fill: rgb(255, 255, 255); stroke: rgb(121, 134, 203); stroke-width: 1.5;"></path><path d="M4.898587196589413e-16,-8A8,8 0 1,1 -6.472135954999579,4.702282018339786L0,0Z" style="fill: rgb(121, 134, 203);"></path></g></g></svg></span>
                </td>
            </tr>
       
        </tbody>
    </table>
</div>